$("ul.errors").html("")
<% if @trading_account_instrument.errors.any? %>
  <% @trading_account_instrument.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
  <% end %>
<% else %>

  if ($('#trading_account_instrument-list').length) {
    $('#trading_account_instrument-list').html("<%= escape_javascript(render 'trading_accounts/trading_account_instruments_grid') %>")
  }

  $("#item-modal").modal("hide")

<% end %>