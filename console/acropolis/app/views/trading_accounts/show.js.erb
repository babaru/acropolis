$('#net-worth-value').html('<%= number_with_precision(@trading_account.net_worth(@trading_date, @exchange), precision: 4) %>');
$('#balance-value').html('<%= render_currency(@trading_account.balance(@trading_date, @exchange)) %>');
$('#profit-value').html('<%= render_currency(@trading_account.profit(@trading_date, @exchange)) %>');
$('#leverage-value').html('<%= number_with_precision(@trading_account.leverage(@trading_date, @exchange), precision: 4) %>');
$('#exposure-value').html('<%= number_with_precision(@trading_account.exposure(@trading_date, @exchange), precision: 4) %>');
$('#tab-content').html('<%= escape_javascript(render(partial: "tab_trades")) %>');

