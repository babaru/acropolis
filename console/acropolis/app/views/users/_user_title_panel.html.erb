      <td class="key"><%= t('activerecord.attributes.user.avatar_updated_at') %></td>
      <td class="value"><%= @user.avatar_updated_at %></td>

            

      <td class="key"><%= t('activerecord.attributes.user.attachment_access_token') %></td>
      <td class="value"><%= @user.attachment_access_token %></td>

            </tr>
  </table>
</div>
