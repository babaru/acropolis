$("ul.errors").html("")
<% if @capital_account.errors.any? %>
  <% @capital_account.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
  <% end %>
<% else %>

  if ($('#capital_account-detail').length) {
    $('#capital_account-detail').html("<%= escape_javascript(render 'capital_account_detail') %>")
  }

  if ($('#capital_account-list').length) {
    $('#capital_account-list').html("<%= escape_javascript(render 'capital_accounts_grid') %>")
  }

  $("#item-modal").modal("hide")

<% end %>